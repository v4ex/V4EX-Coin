name = "v4ex-coin"
# Credentials
account_id = ""
zone_id = ""
# type = "javascript" is required to use the `[build]` section
type = "javascript"
workers_dev = true
route = ""


[build]
command = "npm install && npm run build"
[build.upload]
# The "modules" upload format is required for all projects that export a Durable Object class.
dir="build"
format = "modules"
main = "./worker.mjs"


[vars]
AUTH0_MINER_ROLE_ID = ""
AUTH0_MINTER_ROLE_ID = ""
AUTH0_SERVER_ROLE_ID = ""


[durable_objects]
bindings = [
  {name = "MINING", class_name = "Mining"},
  {name = "MINTING", class_name = "Minting"},
  {name = "SERVING", class_name = "Serving"},
]
